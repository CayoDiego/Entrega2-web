        <table class="table">

            <tr>
                <th>Placa</th>
                <th>Fabricante</th>
                <th>Cor</th>
            </tr>

            <% for(let carro of listaCarros) {  %>
                <tr>
                    <td>
                        <%= carro.placa%>
                    </td>
                    <td>
                        <%= carro.fabricante%>
                    </td>
                    <td>
                        <%= carro.cor%>
                    </td>
                </tr>

            <% } %>

        </table>

    </div>

</body>

</html>